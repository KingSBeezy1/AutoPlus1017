@model AutoPlus1017.Entities.Orders

@{
    ViewData["Title"] = "Детайли";
}

<h1>Детайли на поръчките</h1>

<div>
    <h4>Поръчки</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Users)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Users.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Car)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Car.Brand)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.OrderCreateDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.OrderCreateDate)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Редактирай</a> |
    <a  class="btn btn-primary" asp-action="Index">Върни се назад</a>
</div>
